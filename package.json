{
  "name": "trimetric",
  "version": "0.1.0",
  "description": "An app to monitor trimet stops",
  "main": "index.js",
  "scripts": {
    "build": "cd web && webpack",
    "dist":
      "npm run build && cd web && rm -rf dist && mkdir dist && cp -Rv index.html robots.txt assets bundle.css bundle.js dist/",
    "githooks-install": "precommit install",
    "githooks-remove": "precommit remove",
    "lint": "eslint web/src web/test --ext jsx",
    "precommit": "make lint",
    "start": "cd web/dist && http-server",
    "test": "cd web && karma start --single-run",
    "test-watch": "cd web && karma start",
    "watch": "cd web && webpack-dev-server --debug --host 0.0.0.0 & karma start"
  },
  "author": "Brian Davidson",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "deck.gl": "^4.1.5",
    "deep-equal": "^1.0.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "esformatter-jsx": "^8.0.1",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "fetch-ponyfill": "^4.1.0",
    "file-loader": "^1.1.5",
    "haversine": "^1.0.0",
    "haversine-distance": "^1.1.3",
    "history": "^4.7.2",
    "html-loader": "^0.5.1",
    "husky": "^0.14.3",
    "install": "^0.10.1",
    "isparta-loader": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "js-quantities": "^1.6.1",
    "json-loader": "^0.5.4",
    "karma": "^1.7.1",
